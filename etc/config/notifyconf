
config globals 'globals'
	list event 'temperature.Температура'
	list event 'userlogin.Вход пользователя'
	list event 'userlogout.Выход пользователя'
	list event 'configchanged.Конфигурация изменена'
	list event 'operation.Операция'
	list event 'error.Ошибка'
	list method 'email.Почта'
	list method 'syslog.Журнал'
	list method 'snmptrap.Отправить snmp сообщение'
	list method 'sms.Сообщение SMS'

config notify 'prototype'
	option name 'Notify'
	option state '0'
	option event 'temperature'
	option method 'email'
	option text 'Message text here'
	option sendto 'addr1,addr2'
	option expression '0'
	option timetable 'datetime1-datetime2,datetime3-datetime4'

config notify 'ntf100001'
	option state '1'
	option event 'configchanged'
	option expression '%_name_%="smtpconf"'
	option method 'syslog'
	option loglevel 'notice'
	option logprefix 'Notification'
	option text 'Email config changed'
	option timetable '14/09/2021 00:01:13-16/09/2021 20:11:13'
	option name 'Notify 4'

config notify 'ntf100000'
	option method 'email'
	option state '0'
	option name 'Notify 3'
	option timetable 'datetime1-datetime2,datetime3-datetime4'
	option event 'userlogin'
	option text 'Message text here'
	option expression '0'
	option sendto 'addr1,addr2'

